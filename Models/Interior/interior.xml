<?xml version="1.0" encoding="ISO-8859-1"?> 

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>inttour</object-name>
    <object-name>planchet</object-name>

    <!-- Objets transparents -->
    <object-name>intvitres</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/TU-95/Models/Effects/Glass/glass</inherits-from>
    <object-name>intvitres</object-name>
  </effect>
  
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>intvitres</object-name>
  </effect>
  
  <!-- Pilote -->
  <model>
    <path>Aircraft/TU-95/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -18.154 </x-m>
      <y-m>  -0.400 </y-m>
      <z-m>  -1.731 </z-m>
    </offsets>
  </model>

  <!-- Planche de bord -->
  <model>
    <path>Aircraft/TU-95/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -18.521 </x-m>
      <y-m>   0.000 </y-m>
      <z-m>  -1.394 </z-m>
    </offsets>
  </model>

</PropertyList>
